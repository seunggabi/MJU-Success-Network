<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.seunggabi.mju_success_network"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="15"
        android:targetSdkVersion="25" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:launchMode="singleInstance"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >
        <activity android:name="com.seunggabi.mju_success_network.view.menu.MainActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.seunggabi.mju_success_network.view.menu.GroupActivity"
            android:launchMode="singleInstance" />
        <activity
            android:name="com.seunggabi.mju_success_network.view.menu.ChattingActivity"
            android:launchMode="singleInstance" />
        <activity
            android:name="com.seunggabi.mju_success_network.view.menu.NoticeActivity"
            android:launchMode="singleInstance" />
        <activity
            android:name="com.seunggabi.mju_success_network.view.menu.SettingActivity"
            android:launchMode="singleInstance" />
    </application>

</manifest>